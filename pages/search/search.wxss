@import "../common/user/user.wxss"
@import "../common/repositories/repositories.wxss"
.nav {
	width: 100%;
	height: 100rpx;
	display: flex;
	flex-direction: row;
	border-bottom: 1px solid gainsboro;
}
.default {
	line-height: 100rpx;
	text-align: center;
	flex: 1;
	border-right: 1px solid gainsboro;
	color: #000;
	font-weight: bold;
	font-size: 28rpx;
}
.high-light {
	line-height: 100rpx;
	text-align: center;
	color: #0366d6;
	flex: 1;
	border-right: 1px solid gainsboro;
	font-weight: bold;
	font-size: 28rpx;
}
input {
	padding: 5px;
	border: 1rpx solid gainsboro;
}
.item {
	position:relative;
	display: block;
	text-align: left;
    padding: 20rpx 20rpx 20rpx 60rpx;
    border-top: 1rpx solid #eaecef;
    word-wrap: break-word;
    word-break: break-all;
}
